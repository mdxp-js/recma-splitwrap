{
    "name": "rehype-mdx-splitwrap",
    "version": "0.1.0",
    "description": "Split MDX files on a certain component and wrap the splits",
    "license": "MIT",
    "author": "0phoff <0phoff@users.noreply.github.com>",
    "keywords": [
        "mdx",
        "markdown",
        "rehype",
        "rehype-plugin",
        "rehype-mdx-plugin",
        "mdxp"
    ],
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
        "dist/**/*.js",
        "dist/index.d.ts"
    ],
    "scripts": {
        "build": "tsc",
        "start": "tsc --watch",
        "test-api": "tsx node_modules/uvu/bin.js test \"^(jsx|import)\\.ts$\"",
        "test-cov": "c8 --check-coverage --90 --reporter lcov pnpm run test-api",
        "test": "pnpm run build && pnpm run test-cov",
        "prepack": "npm run build"
    },
    "devDependencies": {
        "@mdx-js/mdx": "^2.1.2",
        "@types/estree-jsx": "^0.0.2",
        "@types/hast": "^2.3.4",
        "@types/node": "^18.0.1",
        "@types/unist": "^2.0.6",
        "c8": "^7.11.3",
        "mdast-util-mdx": "^2.0.0",
        "tsx": "^3.7.1",
        "typescript": "^4.7.4",
        "uvu": "^0.5.6"
    },
    "dependencies": {
        "unified": "^10.1.2",
        "unist-util-walker": "^1.0.0"
    }
}
